{
  "name": "vue-ssr",
  "description": "A Vue.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "node ./server/server.js",
    "start": "cross-env NODE_ENV=production node ./server/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "test": "rimraf dist && npm run test:client && npm run test:server",
    "test:client": "cross-env NODE_ENV=test webpack --config build/webpack.client.config.js --progress --hide-modules",
    "test:server": "cross-env NODE_ENV=test webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
